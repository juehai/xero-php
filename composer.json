{
    "name": "ravebuild/xero-php",
    "type": "library",
    "description": "A client implementation of the Xero API, with a cleaner OAuth interface and ORM-like abstraction. With Ravebuild fixes.",
    "homepage": "https://github.com/juehai/xero-php",
    "license": "MIT",
   "require": {
      "php": ">=5.6.0",
      "calcinai/oauth2-xero": "^1.0",
      "guzzlehttp/guzzle": "^6.5",
      "guzzlehttp/psr7": "^1.5",
      "ext-json": "*",
      "ext-simplexml": "*"
   },
   "require-dev": {
      "phpunit/phpunit": "^5"
   },
   "autoload": {
      "psr-4": {
         "XeroPHP\\": "src/XeroPHP/"
      }
  },
   "autoload-dev": {
      "psr-4": {
         "XeroPHP\\Tests\\": "tests/"
      }
   }
}
